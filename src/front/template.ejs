<!DOCTYPE html>
<html lang="en">
  <head>
    <% if (NODE_ENV === 'production') { %><script async src="https://www.googletagmanager.com/gtag/js?id=UA-67002303-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', 'UA-67002303-1');
    </script><% } %>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <% if (desc) { %><meta name="description" content="<%= desc %>"><% } %>
    <% if (keywords) { %><meta name="keywords" content="<%= keywords.toString() %>"><% } %>
    <% if (css) { %><link rel="stylesheet" href="<%= css %>" /><% } %>
    <title><% if (title) { %><%- title %><% } %></title>
  </head>
  <body>
    <noscript>You need to enable JavaScript to run this website properly.</noscript>
    <div id="root"><% if (app) { %><%- app %><% } %></div>
    <script src="<%= js %>"></script><% if (NODE_ENV === 'development') { %><script src="/dev.js"></script><% } %>
  </body>
</html>
