{
  "name": "mattlean.com-2020",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/mattlean/mattlean.com-2020.git",
  "author": "Matthew Lean <matt@mattlean.com>",
  "private": true,
  "scripts": {
    "back:build": "webpack --config webpack/back/config.js --env.type development",
    "back:build:production": "webpack --config webpack/back/config.js --env.type production",
    "back:start": "NODE_ENV=development node build/back/development/server",
    "back:start:production": "NODE_ENV=production node build/back/production/server",
    "build:production": "yarn front:build:production && yarn back:build:production",
    "dev": "nodemon --exec \"yarn front:build && yarn back:build && yarn back:start\"",
    "front:build": "webpack --config webpack/front/config.js --env.type development",
    "front:build:production": "webpack --config webpack/front/config.js --env.type production",
    "format": "prettier --write .",
    "start:production": "NODE_ENV=production node build/back/production/server"
  },
  "dependencies": {
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "framer-motion": "^1.11.1",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
