{
  "name": "mattlean.com-2020",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/mattlean/mattlean.com-2020.git",
  "author": "Matthew Lean <matt@mattlean.com>",
  "private": true,
  "scripts": {
    "back:build": "webpack --config webpack/back/config.js --env.type development",
    "back:build:production": "webpack --config webpack/back/config.js --env.type production",
    "back:start": "NODE_ENV=development node build/back/development/server",
    "back:start:production": "NODE_ENV=production node build/back/production/server",
    "back:start:debug": "nodemon --exec \"yarn front:build && yarn back:build && NODE_ENV=development node --inspect-brk build/back/development/server\"",
    "build": "yarn front:build:production && yarn back:build:production",
    "dev": "nodemon --exec \"yarn front:build && yarn back:build && yarn back:start\"",
    "front:dev": "nodemon --config nodemon-front.json --exec \"webpack-dev-server --config webpack/front/config.js --env.type development --env.devType front\"",
    "front:build": "webpack --config webpack/front/config.js --env.type development",
    "front:build-dev": "webpack --config webpack/front/config.js --env.type development --env.devType front",
    "front:build:production": "webpack --config webpack/front/config.js --env.type production",
    "format": "prettier --write src/ webpack/",
    "lint": "yarn lint:styles; yarn lint:js",
    "lint:js": "eslint --ext .js,.json,.jsx src/ webpack/",
    "lint:styles": "stylelint \"src/**/*.{css,scss}\"",
    "sass:build": "yarn sass --load-path=node_modules/eswiss/dist src/front/index.scss:build/front/sass/index.css",
    "start": "NODE_ENV=production node build/back/production/server"
  },
  "dependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@primer/octicons-react": "^10.0.0",
    "@svgr/webpack": "^5.4.0",
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "ejs": "^3.1.3",
    "eswiss": "^0.0.30",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "framer-motion": "^2.6.5",
    "helmet": "^4.1.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^2.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  }
}
